        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kayschima\\DeutscheBahn\\DeutscheBahnServiceProvider"
            ],
            "aliases": {
                "DeutscheBahn": "Kayschima\\DeutscheBahn\\Facades\\DeutscheBahn"
            }
        }
    }
