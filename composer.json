{
    "name": "kayschima/deutsche-bahn",
    "description": "Laravel package for Deutsche Bahn timetables",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kay Markschies",
            "email": "kayschima@web.de",
            "homepage": "https://markschies.com"
        }
    ],
    "homepage": "https://github.com/kayschima/deutsche-bahn",
    "keywords": [
        "Laravel",
